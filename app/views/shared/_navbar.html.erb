<div class="navbar navbar-expand-sm navbar-light navbar-lewagon">
  <%= link_to profile_path, class: "navbar-brand" do %>
    <%= image_tag "https://raw.githubusercontent.com/lewagon/fullstack-images/master/uikit/logo.png" %>
  <% end %>

  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>


  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
      <li> <%= link_to "Home", "#", class: "nav-link text-light" %> </li>
      <li> <%= link_to "About", "#", class: "nav-link text-light"  %> </li>
      <li> <%= link_to "Suport", "#", class: "nav-link text-light mr-3"  %> </li>
      <% if user_signed_in? %>
        <li class="nav-item active">
          <%= link_to "Log out", destroy_user_session_path, method: :delete, class: "btn btn-danger" %>
        </li>
      <% else %>
          <%= link_to "Login", new_user_session_path, class: "btn btn-danger" %>
      <% end %>
    </ul>
  </div>
</div>
